\title{Macros}

% Category theory
\def\cat[x]{#{\mathbf{#x}}}
\def\Set{\cat{Set}}
\def\Top{\cat{Top}}
\def\Grp{\cat{Grp}}
\def\Cat{\cat{Cat}}

% Common symbols
\def\N{#{\mathbb{N}}}
\def\Z{#{\mathbb{Z}}}
\def\Q{#{\mathbb{Q}}}
\def\R{#{\mathbb{R}}}
\def\C{#{\mathbb{C}}}

% Arrows
\def\to{#{\rightarrow}}
\def\mapsto{#{\mapsto}}
\def\iso{#{\cong}}
